{
  "name": "vurif",
  "description": "Vue component dependency analyzer for Vue.js 2",
  "version": "0.2.0",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonNozaki/vurif.git"
  },
  "bin": {
    "vurif": "./bin/vurif.mjs"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "bin",
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "unbuild",
    "dev": "npx jiti playground/cli.ts",
    "lint": "biome lint",
    "test": "vitest"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "cytoscape": "^3.30.1",
    "mustache": "^4.2.0",
    "unbuild": "3.0.0-rc.7",
    "vue": "2.7.16",
    "vue-template-compiler": "^2.7.16"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@types/cytoscape": "^3.21.5",
    "@types/mock-fs": "^4.13.4",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.14.12",
    "jiti": "^1.21.6",
    "mock-fs": "^5.2.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "packageManager": "pnpm@9.6.0",
  "keywords": ["Vue", "Nuxt", "CLI"]
}
